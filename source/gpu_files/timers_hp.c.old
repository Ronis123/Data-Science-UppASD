/**********************************************************************/
/* timef for linux - return millisecond wall clock time		      */
/**********************************************************************/
#include <sys/types.h>
#include <sys/time.h>
double timef_()
{
  double msec;
  struct tv;
  gettimeofday(&tv,0);
  msec = tv.tv_sec * 1.0e3 + tv.tv_usec * 1.0e-3;
  return msec;
}
/**********************************************************************/
/* second for linux - return second CPU clock time		      */
/**********************************************************************/
float second_()
{
  return (float) clock()/CLOCKS_PER_SEC;
}

